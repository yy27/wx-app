page{
    width: 100%;
    height: 100%;
    font-size:30rpx;
    font-family: "微软雅黑"
}
@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: 500;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-comments:before { content: "\e69b"; }

.icon-process:before { content: "\e6ce"; }

.icon-bad:before { content: "\e716"; }

.icon-good:before { content: "\e717"; }
.time{
    font-size: 25rpx;
    color: #999;
}
.top{
    height: 100rpx;
    width: 100%;
    display: flex;
    align-items: center;
}
.face{
    flex: 1;
}
.face image{
    width: 90rpx;
    height: 90rpx;
    border-radius: 100%;
    border:1px solid #eee;
}
.name{
    flex: 6;
}

.video{
    margin-top:20rpx;
    width: 100%;
    position: relative;
}
.video video{
    width:100%;
    z-index: -1;
}
.imgwrap{
    width: 100%;
}
.video .img{
    width: 100%;
    background-color: #000;
    text-align: center;
    min-height:450rpx;
    display: flex;
    align-items: center;
    position: relative;
}
.videoTar{
    position: absolute;
    left: 0;
    top:0;
    width: 100%;
    height: 100%;
}
.videoTar video{
    width: 100%;
    height: 100%;
}
.video .img image{
    margin:0 auto;
}
.video .img .play-btn image{
    width: 100rpx;
    height: 100rpx;
    margin-left:-50rpx;
    margin-top:-50rpx;
}
.play-btn{
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    left:50%;
    top:50%;
}
.img-wrap{
    max-width:100%;
    
    text-align: center;
    position: relative;
}
.hidden{
    max-height:500rpx;
    overflow: hidden;
}
.open-more{
    position: absolute;
    left:0;
    bottom: 0;
    width: 100%;
    background: rgba(0, 0, 0, 0.4);
    height: 80rpx;
    color: #fff;
    line-height: 80rpx;
}
.audio audio{
    margin:20rpx 0;
}
.bottom{
    width: 100%;
    height: 80rpx;
    display: flex;
    align-items: center;
    color: #999;
}
.bottom .b-num{
    flex: 1;
    text-align: center;
    line-height: 80rpx;
}
.bottom .b-num .iconfont{
    font-size: 35rpx;
    margin-right:10rpx;
}
.loading{
    height: 80rpx;
    line-height: 80rpx;
    color: #999;
    text-align: center;
}